# Description: Terminal Interface Construction Kit.
# URL:         http://www.leonerd.org.uk/code/libtickit 
# Maintainer:  6c37 Team, https://github.com/6c37/crux-ports/issues
# Depends on:  libtermkey

name=libtickit
version=486
release=1
source=(http://www.leonerd.org.uk/code/$name/$name-0.1-RC2+bzr$version.tar.gz)

build() {
	cd $name-0.1-RC2+bzr$version

	make PREFIX=/usr
	make PREFIX=/usr DESTDIR=$PKG install
}
